<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean name="person" class="cn.bruce.spring.basic_di.a_quickstart_set.bean.Person">
        <property name="name" value="zhangsan"></property>
        <property name="age" value="18"></property>
    </bean>

    <bean name="cat" class="cn.bruce.spring.basic_di.a_quickstart_set.bean.Cat">
        <property name="name" value="huahua"></property>
        <property name="master" ref="person"></property>
    </bean>
</beans>